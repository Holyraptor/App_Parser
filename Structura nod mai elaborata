#include <stdio.h>
#include <conio.h>
#include <stdlib.h>
#include <stdarg.h>
#include <string.h>
#include <ctype.h>
#include <time.h>

#ifndef ARBORE_BINAR
#define ARBORE_BINAR
#define Frunza(a)(((a)->st==NULL)&&((a)->dr==NULL));
#define Interior(a) (((a)->st != NULL ) || ((a)->dr !=NULL))
#define Ordin(a) (((a)->st != NULL ) + ((a)->dr != NULL))

typedef struct nod 
{ 
  struct nod*st, *dr;
  char* radacina;
  double start, end;
  char variable;
  double rez; //Optional
} TNod, *TArb, **AArb;
